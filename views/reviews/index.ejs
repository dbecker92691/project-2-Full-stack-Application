<!DOCTYPE html>
<html>
<head>
    <% include ../partials/csshead %>
</head>
<body>
  <header>
    <% include ../partials/navbar %>
  </header>
  <main>
    <h1>This is our Reviews Page </h1><br>
  </main>
  <body>
    <% for(let i = 0; i < happy.length; i++) { %>
      <ul>
          <li>
            <a href='happyhours/<%=happy._id%>'>
              <%=happy[i].restaurant%>
            </a>
          </li>
      </ul>
    <% } %>
    <a href='reviews/new'>New Review</a>
    <% for(let i = 0; i < reviews.length; i++) { %>
      <ul>
        <li>
          <%= reviews[i].user %><br/>
          <%= reviews[i].restaurant %><br/>
          <%= reviews[i].body %>
        </li>
      </ul>
    <%}%>
  </body>

</body>
</html>